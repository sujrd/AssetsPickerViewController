ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AssetsPickerViewController" "${PODS_CONFIGURATION_BUILD_DIR}/Device" "${PODS_CONFIGURATION_BUILD_DIR}/Dimmer" "${PODS_CONFIGURATION_BUILD_DIR}/FadeView" "${PODS_CONFIGURATION_BUILD_DIR}/PureLayout" "${PODS_CONFIGURATION_BUILD_DIR}/SwiftARGB" "${PODS_CONFIGURATION_BUILD_DIR}/TinyLog" "${PODS_CONFIGURATION_BUILD_DIR}/UIViewKVO"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "${PODS_CONFIGURATION_BUILD_DIR}/AssetsPickerViewController/AssetsPickerViewController.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Device/Device.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Dimmer/Dimmer.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/FadeView/FadeView.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/PureLayout/PureLayout.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/SwiftARGB/SwiftARGB.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/TinyLog/TinyLog.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/UIViewKVO/UIViewKVO.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "AssetsPickerViewController" -framework "Device" -framework "Dimmer" -framework "FadeView" -framework "PureLayout" -framework "SwiftARGB" -framework "TinyLog" -framework "UIViewKVO"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
